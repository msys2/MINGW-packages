post_install() {
  cd mingw32
  local _prefix=$(pwd -W)
  cd -
  for f in mingw32/lib/cmake/PySide2-*/*.cmake; do
    # Do it twice if we already process one of the files
    sed -e "s|${_prefix}|/mingw32|g" -i ${f}
    sed -e "s|/mingw32|${_prefix}|g" -i ${f}
  done
}

post_upgrade() {
  post_install
}
