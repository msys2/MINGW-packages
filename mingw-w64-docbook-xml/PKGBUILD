  ${MINGW_PREFIX}/bin/xmlcatalog --noout --add "public" \
    "-//OASIS//DTD XML Exchange Table Model 19990315//EN" \
    "http://www.oasis-open.org/docbook/xml/4.3/soextblx.dtd" \
    "${pkgdir}${MINGW_PREFIX}/etc/xml/docbook-xml"

  ${MINGW_PREFIX}/bin/xmlcatalog --noout --add "public" \
