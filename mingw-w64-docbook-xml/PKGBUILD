  ${MINGW_PREFIX}/bin/xmlcatalog --noout --add "public" \
    "-//OASIS//ENTITIES DocBook Additional General Entities V4.2//EN" \
    "http://www.oasis-open.org/docbook/xml/4.2/dbgenent.mod" \
    "${pkgdir}${MINGW_PREFIX}/etc/xml/docbook-xml"

