post_install() {
  _mingw=mingw64
  cd ${_mingw}
  local _prefix=$(pwd -W)
  cd -
  sed -e "s|/${_mingw}|${_prefix}|g" -i	${_mingw}/lib/oce/OCE-libraries.cmake
  sed -e "s|/${_mingw}|${_prefix}|g" -i	${_mingw}/lib/oce/OCE-libraries-release.cmake
  sed -e "s|/${_mingw}|${_prefix}|g" -i	${_mingw}/lib/oce/OCEConfig.cmake
}

post_upgrade() {
  post_install
}
