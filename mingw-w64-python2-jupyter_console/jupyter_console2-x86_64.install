post_install() {
  cd mingw64
  local _prefix=$(pwd -W)
  cd -
  local _it
# "somepackage" should be replaced with the name of your .EXE w/o 2 and .exe ext
# Like this:
# For "myexe2.exe", it would be "myexe2" .
  for _it in jupyter-console2; do
    sed -e "s|/mingw64|${_prefix}|g" \
        -i mingw64/bin/${_it}
  done
}

post_upgrade() {
  post_install
}
