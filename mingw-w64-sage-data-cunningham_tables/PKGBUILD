# Contributor: Dirk Stolle

_dbname=cunningham_tables
_realname=sage-data-${_dbname}
pkgbase=mingw-w64-${_realname}
pkgname=("${MINGW_PACKAGE_PREFIX}-${_realname}")
pkgver=1.0
pkgrel=1
pkgdesc="List of prime numbers occuring in the Cunningham table (mingw-w64)"
arch=('any')
mingw_arch=('mingw64' 'ucrt64' 'clang64' 'clangarm64')
url='https://www.sagemath.org'
msys2_repository_url='https://github.com/sagemath/sage-package'
msys2_references=(
  'archlinux: sage-data-cunningham_tables'
)
license=('spdx:GPL-2.0-or-later')
depends=()
source=("https://github.com/sagemath/sage-package/releases/download/tars/${_dbname}-${pkgver}.tar.gz")
sha256sums=('ef39ab25bef5b1813071c9bd96abe3a9e683d5595c9654a3ffde5b07b7fe52b0')

package() {
  cd "${_dbname}-${pkgver}/src"

  install -d "${pkgdir}${MINGW_PREFIX}/share"
  cp -r ${_dbname} "${pkgdir}${MINGW_PREFIX}/share"
}
