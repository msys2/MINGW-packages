From a96e3fe08f4f790d1ae217858a23f76487844069 Mon Sep 17 00:00:00 2001
From: Alethea Rose <alethea@alethearose.com>
Date: Tue, 14 Feb 2017 23:11:03 -0500
Subject: [PATCH 05/15] Remove .lib suffix on linked libraries

---
 deps/v8/BUILD.gn         | 4 ++--
 deps/v8/tools/gyp/v8.gyp | 2 +-
 node.gypi                | 2 +-
 3 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/deps/v8/BUILD.gn b/deps/v8/BUILD.gn
index 28aca24a..c8d35370 100644
--- a/deps/v8/BUILD.gn
+++ b/deps/v8/BUILD.gn
@@ -1902,8 +1902,8 @@ source_set("v8_libbase") {
     defines += [ "_CRT_RAND_S" ]  # for rand_s()
 
     libs = [
-      "winmm.lib",
-      "ws2_32.lib",
+      "winmm",
+      "ws2_32",
     ]
   }
 
diff --git a/deps/v8/tools/gyp/v8.gyp b/deps/v8/tools/gyp/v8.gyp
index 83230750..79de18f9 100644
--- a/deps/v8/tools/gyp/v8.gyp
+++ b/deps/v8/tools/gyp/v8.gyp
@@ -1910,7 +1910,7 @@
               ],
               'msvs_disabled_warnings': [4351, 4355, 4800],
               'link_settings':  {
-                'libraries': [ '-lwinmm.lib', '-lws2_32.lib' ],
+                'libraries': [ '-lwinmm', '-lws2_32' ],
               },
             }],
           ],
diff --git a/node.gypi b/node.gypi
index 3b3548fd..fe85f1f3 100644
--- a/node.gypi
+++ b/node.gypi
@@ -262,7 +262,7 @@
         'NODE_PLATFORM="win32"',
         '_UNICODE=1',
       ],
-      'libraries': [ '-lpsapi.lib' ]
+      'libraries': [ '-lpsapi' ]
     }, { # POSIX
       'defines': [ '__POSIX__' ],
       'sources': [ 'src/backtrace_posix.cc' ],
-- 
2.17.0.windows.1

