# Order is important, primary package first
declare -rga _realname=(perl perl-doc)
pkgname=()
declare -gA _pkgname=()
local _n _p
for _n in "${_realname[@]}"; do
    _p="${MINGW_PACKAGE_PREFIX}-${_n}"
    _pkgname["${_n}"]="${_p}"
    pkgname+=("${_p}")
done
readonly _pkgname
pkgbase="mingw-w64-${_realname}"
